{
    "name": "bim-gdpr",
    "version": "1.1.0",
    "description": "Easy way to propose personal data services (like cookie) manager",
    "main": "./src/bgdpr.js",
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "bgdpr": "node ./contrib/forge/forge.js",
        "postinstall": "node ./contrib/post_install.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tsecher/bim-gdpr.git"
    },
    "keywords": [
        "gdpr",
        "cookies",
        "rgpd",
        "personal data manager"
    ],
    "author": "Thomas SÃ©cher",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tsecher/bim-gdpr/issues"
    },
    "homepage": "https://github.com/tsecher/bim-gdpr#readme",
    "dependencies": {
        "glob": "^7.1.6",
        "js-cookie": "^2.2.1",
        "prompts": "^2.3.2",
        "scriptjs": "^2.5.9"
    },
    "devDependencies": {
        "laravel-mix-easy-structure": "git+https://thomasse@bitbucket.org/thomasse/laravel-mix-easy-structure.git#1.0-dev",
        "matomo-tracker": "^2.2.1",
        "prompt-choices": "^5.0.0",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    }
}
